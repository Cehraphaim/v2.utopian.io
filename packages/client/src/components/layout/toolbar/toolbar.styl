.q-toolbar.u-toolbar {
  height 60px
  div.toolbar-container {
    justify-content space-between
    align-items center
    display flex
  }
  div.u-toolbar-right {
    > * {
      margin-left 16px
    }
    img.avatar {
      height 32px
      width 32px
      border-radius 50%
      border: 2px solid rgba(white, 0.6)
    }
  }
}
